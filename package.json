{
    "name": "vet",
    "version": "2.17.2",
    "description": "Veterinary",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "rm -rf dist && electron-builder --x64 --win --publish always"
    },
    "build": {
        "appId": "com.binariez.Ja3farMortada.vet-electron",
        "productName": "Veterinary",
        "artifactName": "${name}-${version}.${ext}",
        "target": "NSIS",
        "win": {
            "target": "nsis"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "Ja3farMortada",
                "repo": "vet-electron",
                "releaseType": "release"
            }
        ],
        "npmRebuild": "false",
        "directories": {
            "buildResources": "resources"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ja3farMortada/vet-electron.git"
    },
    "author": "Binariez LLC",
    "license": "ISC",
    "dependencies": {
        "angular": "^1.8.3",
        "bootstrap": "^5.3.8",
        "electron-context-menu": "^3.6.1",
        "electron-log": "^5.4.3",
        "electron-updater": "^6.6.2",
        "jsbarcode": "^3.12.1",
        "moment": "^2.30.1"
    },
    "devDependencies": {
        "electron": "^39.2.6",
        "electron-builder": "^26.0.12"
    }
}