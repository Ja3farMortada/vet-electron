{
    "name": "veterinary",
    "version": "2.8.0",
    "description": "Veterinary Electron",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-builder --x64"
    },
    "build": {
        "appId": "com.binariez.Ja3farMortada.vet-electron",
        "productName": "Veterinary",
        "target": "NSIS",
        "publish": [
            {
